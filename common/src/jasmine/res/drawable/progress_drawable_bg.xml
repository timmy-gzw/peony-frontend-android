<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="9dp" />
            <gradient
                android:angle="270"
                android:centerColor="#33FEFBEE"
                android:centerY="0.75"
                android:endColor="#33FEFBEE"
                android:startColor="#33FEFBEE" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="9dp" />
                <gradient
                    android:centerColor="#FEFBEE"
                    android:endColor="#373F46"
                    android:startColor="#1E262E" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="9dp" />
                <gradient
                    android:endColor="#FFECC1"
                    android:startColor="#FFB74E" />
            </shape>
        </clip>
    </item>

</layer-list>