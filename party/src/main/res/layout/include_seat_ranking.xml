<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:id="@+id/con_root_seat_ranking"
        android:layout_height="wrap_content">
        <!-- 红方 -->
        <!-- 座位1-->
        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/img_party_room_seat_not"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:background="@drawable/shape_bg_white_alpha40"
            android:layout_marginBottom="3dp"
            android:src="@drawable/party_room_seat_not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:riv_border_color="@color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="@dimen/dp_100" />

        <ImageView
            android:id="@+id/img_party_room_seat_1_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/party_room_seat_not1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/img_party_room_seat_not"
            app:layout_constraintRight_toRightOf="@id/img_party_room_seat_not" />

        <!-- 座位2-->
        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/img_party_room_seat_not2"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginStart="@dimen/dp_8"
            android:background="@drawable/shape_bg_white_alpha40"
            android:layout_marginBottom="3dp"
            android:src="@drawable/party_room_seat_not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/img_party_room_seat_not"
            app:riv_border_color="@color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="@dimen/dp_100" />

        <ImageView
            android:id="@+id/img_party_room_seat_2_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/party_room_seat_not2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/img_party_room_seat_not2"
            app:layout_constraintRight_toRightOf="@id/img_party_room_seat_not2" />

        <!-- 座位3-->
        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/img_party_room_seat_not3"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginStart="@dimen/dp_8"
            android:background="@drawable/shape_bg_white_alpha40"
            android:layout_marginBottom="3dp"
            android:src="@drawable/party_room_seat_not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/img_party_room_seat_not2"
            app:riv_border_color="@color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="@dimen/dp_100" />

        <ImageView
            android:id="@+id/img_party_room_seat_3_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/party_room_seat_not3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/img_party_room_seat_not3"
            app:layout_constraintRight_toRightOf="@id/img_party_room_seat_not3" />


        <!-- 蓝方 -->
        <!-- 座位1-->
        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/blue_img_party_room_seat_not"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:background="@drawable/party_room_seat_blue_not"
            android:layout_marginBottom="3dp"
            android:src="@drawable/party_room_seat_blue_not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:riv_border_color="@color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="@dimen/dp_100" />

        <ImageView
            android:id="@+id/blue_img_party_room_seat_1_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/party_room_seat_blue_not1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/blue_img_party_room_seat_not"
            app:layout_constraintRight_toRightOf="@id/blue_img_party_room_seat_not" />

        <!-- 座位2-->
        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/blue_img_party_room_seat_not2"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginEnd="@dimen/dp_8"
            android:background="@drawable/party_room_seat_blue_not"
            android:layout_marginBottom="3dp"
            android:src="@drawable/party_room_seat_blue_not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/blue_img_party_room_seat_not"
            app:riv_border_color="@color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="@dimen/dp_100" />

        <ImageView
            android:id="@+id/blue_img_party_room_seat_1_number2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/party_room_seat_blue_not2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/blue_img_party_room_seat_not2"
            app:layout_constraintRight_toRightOf="@id/blue_img_party_room_seat_not2" />

        <!-- 座位3-->
        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/blue_img_party_room_seat_not3"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginEnd="@dimen/dp_8"
            android:layout_marginBottom="3dp"
            android:src="@drawable/party_room_seat_blue_not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/blue_img_party_room_seat_not2"
            app:riv_border_color="@color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="@dimen/dp_100" />

        <ImageView
            android:id="@+id/blue_img_party_room_seat_1_number3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/party_room_seat_blue_not3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/blue_img_party_room_seat_not3"
            app:layout_constraintRight_toRightOf="@id/blue_img_party_room_seat_not3" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>